{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "api/index.py",
      "methods": ["GET"]
    },
    {
      "src": "/check_authenticity",
      "dest": "api/index.py",
      "methods": ["POST"]
    },
    {
      "src": "/(.*)",
      "dest": "api/index.py"
    }
  ]
}
